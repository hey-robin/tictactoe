{
    "extends": ["eslint:recommended", "react-app"],
    "rules": {
        "no-constant-condition": "off",
        "no-console": "warn",
        "indent": ["error", 2],
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "space-in-parens": ["error"],
        "object-curly-spacing": ["error", "always"],
        "react/jsx-curly-spacing": [2, {"when": "never", "spacing": {
            "objectLiterals": "never"
        }}]
    }
}